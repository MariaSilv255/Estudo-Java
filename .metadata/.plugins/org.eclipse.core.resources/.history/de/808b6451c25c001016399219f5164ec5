package data_hora;

import java.time.Instant;
import java.time.LocalDate;
import java.time.LocalDateTime;

public class calculso {
	public static void main(String[] args) {
		LocalDate d1 = LocalDate.parse("2022-02-01");
		LocalDateTime d3 = LocalDateTime.parse("2022-02-01T05:32:55");
		Instant d2 = Instant.parse("2022-02-01T01:30:26Z");
		
		LocalDate semana_anterior = d1.minusDays(7);
		LocalDate semana_proxima = d1.plusDays(7);
		  System.out.println("d1"+ semana_anterior);
		  System.out.println("d2"+ semana_proxima);
		
	}
	
	
	
		
}
