package data_hora;

import java.time.LocalDate;
import java.time.LocalDateTime;

public class organizando_as_ideaias {
	public static void main(String[] args) {
		
		//no formato iso 8601
		//datas local
		LocalDate data1 = LocalDate.parse("2025-07-09"); //formato padrao
		LocalDateTime data2 = LocalDateTime.parse("2025-07-09T10:36:17"); //formato padrao
		System.out.println(data1);
		System.out.println(data2);
		
		

	}
}
